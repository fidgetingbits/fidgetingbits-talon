app: hexrays_vault
-

expand: key(right)
expand (all | tree): key(ctrl-keypad_plus)
collapse: key(left)
collapse (all | tree): key(ctrl-keypad_minus)
flip: key(ctrl+tab)
file refresh: key(f5)
hunt files: key(ctrl-alt-f)

file add: key(ctrl-a)
file edit: key(ctrl-e)
file open: key(ctrl-o)
file history: key(ctrl-h)
file commit: key(ctrl-s)
file get [latest]: key(ctrl-l)
file checkout: key(alt-p)

file merge [interactive]: key(alt-r)
